<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.evergrande.qms.mapper.QmsOrderMaterialDetailBindMapper" >
<resultMap id="BaseResultMap" type="com.evergrande.qms.model.Material">
    <id column="id" jdbcType="VARCHAR" property="id" />
    <result column="inspection_order_id" jdbcType="VARCHAR" property="inspectionOrderId" />
    <result column="sample_plan_detail_id" jdbcType="VARCHAR" property="samplePlanDetailId" />
    <result column="create_user" jdbcType="TIMESTAMP" property="createUser" />
    <result column="create_date" jdbcType="VARCHAR" property="createDate" />
    <result column="update_user" jdbcType="TIMESTAMP" property="updateUser" />
    <result column="update_date" jdbcType="VARCHAR" property="updateDate" />
</resultMap>
    <sql id="Base_Column_List" >
        id, inspection_order_id, sample_plan_detail_id, create_user, create_date, update_user, update_date
    </sql>

    <select id="selectInspectionOrderId" resultMap="BaseResultMap" parameterType="java.lang.String" >
        SELECT
        <include refid="Base_Column_List" />
        FROM qms_order_material_detail_bind
        where sample_plan_detail_id = #{id,jdbcType=VARCHAR}
    </select>
</mapper>